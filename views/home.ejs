      .text-copy {
        fill: none;
        stroke: white;
        stroke-dasharray: 6% 29%;
        stroke-width: 8px;
        stroke-dashoffset: 0%;
        animation: stroke-offset 6.5s infinite linear;
      }

      .text-copy:nth-child(1) {
        stroke: #1081eb;
        animation-delay: -1;
      }

      .text-copy:nth-child(2) {
        stroke: #1081eb;
        animation-delay: -2s;
      }

      .text-copy:nth-child(3) {
        stroke: #1081eb;
        animation-delay: -3s;
      }

      .text-copy:nth-child(4) {
        stroke: #1081eb;
        animation-delay: -4s;
      }

      .text-copy:nth-child(5) {
        stroke: #1081eb;
        animation-delay: -5s;
      }
      .text-copy:nth-child(6) {
        stroke: #1081eb;
        animation-delay: -6s;
      }
      .text-copy:nth-child(7) {
        stroke: #1081eb;
        animation-delay: -7s;
      }

      @keyframes stroke-offset {
        100% {
          stroke-dashoffset: -35%;
        }
      }

